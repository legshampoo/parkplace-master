    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-transform-catch-errors": "^1.0.2",
